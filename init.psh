#This file is configuration file for ipsec on packetngin.
#Move this file and main file to firmware directory.
#GW1
#VLan Manage
vlan add eth0 10
vlan add eth0 101

#VM Allocate
$vmid = create core: 7 memory: 0x8000000 storage: 0x4000000 nic: mac: 0 dev: eth0.101 ibuf: 1024 obuf: 1024 iband: 1000000000 oband: 1000000000 hpad: 64 tpad: 64 pool: 0x40000000 nic: mac: 0 dev: eth0.10 ibuf: 1024 obuf: 1024 iband: 1000000000 oband: 1000000000 hpad: 64 tpad: 64 pool: 0x40000000 args: hello world
upload $vmid main
start $vmid
#Manage IP
stdin $vmid 0 "ip add eth0 192.168.101.254"
stdin $vmid 0 "ip add eth1 10.0.0.1"

#Manage SP
stdin $vmid 0 "sp add eth0 -p any -s 192.168.101.0/24:0 -d 192.168.200.0/24:0 -a ipsec/out -i 0 -o eth1"
stdin $vmid 0 "content add eth0 out 0 -m tunnel 10.0.0.1-10.0.0.2 -E 3des_cbc -A hmac_sha512 -i 0"

stdin $vmid 0 "sp add eth1 -p any -s 192.168.200.0/24:0 -d 192.168.101.0/24:0 -a ipsec/in -i 0 -o eth0"
stdin $vmid 0 "content add eth1 in 0 -m tunnel 10.0.0.2-10.0.0.1 -E 3des_cbc -A hmac_sha512 -i 0"

#Manage Content

#Manage SA
stdin $vmid 0 "sa add eth0 -p any -s 192.168.101.0/24:0 -d 192.168.200.0/24:0 -m tunnel 10.0.0.1-10.0.0.2 -spi 0x200 -E 3des_cbc 0x0123456789abcdef0123456789abcdef0123456789abcdef -A hmac_sha512 0x0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef"
stdin $vmid 0 "sa add eth1 -p any -s 192.168.200.0/24:0 -d 192.168.101.0/24:0 -m tunnel 10.0.0.2-10.0.0.1 -spi 0x201 -E 3des_cbc 0x0123456789abcdef0123456789abcdef0123456789abcdef -A hmac_sha512 0x0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef"

